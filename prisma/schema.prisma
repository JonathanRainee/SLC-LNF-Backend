generator client{
    provider = "prisma-client-js"
}

datasource db{
    provider = "mysql"
    url = env("DATABASE_URL")
}

model Item{
    id          String       @id @default(uuid())
    name        String
    type        String
    foundAt     Int
    foundDate   DateTime
    description String
    createdAt   DateTime     @default(now())
    updatedAt   DateTime     @updatedAt
}